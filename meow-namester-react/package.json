    "cssnano-preset-advanced": "^7.0.6",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-custom-properties": "^14.0.4",
    "postcss-merge-rules": "^7.0.4",
    "postcss-preset-env": "^10.1.3",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.4.2"
  }
}
