{
  "name": "meow-namester-react",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "css:optimize": "cp src/index.css src/index.css.backup && postcss src/index.css --map --output src/index.optimized.css && prettier --write src/index.optimized.css",
    "css:optimize-all": "find src -name '*.css' -exec sh -c 'cp \"{}\" \"{}.backup\" && postcss \"{}\" --map --output \"{}.temp\" && prettier --write --parser css \"{}.temp\" && mv \"{}.temp\" \"{}\"' \\;",
    "css:find-dupes": "node find-css-dupes.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@hello-pangea/dnd": "^17.0.0",
    "@supabase/supabase-js": "^2.47.3",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
